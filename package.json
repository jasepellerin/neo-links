{
	"name": "neo-links",
	"description": "Neo Links",
	"version": "1.0.0",
	"main": "src/index.tsx",
	"license": "MIT",
	"homepage": "https://jasepellerin.github.io/neo-links",
	"scripts": {
		"dev:base": "webpack-cli serve --mode=development --env development --hot",
		"dev": "nodemon --watch webpack.config.ts --watch package.json --ext ts,json --exec \"yarn dev:base\"",
		"build": "webpack --mode=production --env production --progress",
		"lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
		"lint:fix": "eslint './src/**/*.{ts,tsx,js,jsx}' --fix",
		"format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
		"predeploy": "yarn build",
		"deploy": "gh-pages -d dist",
		"styles": "tailwindcss -i ./src/styles.css -o ./public/styles.css --watch"
	},
	"devDependencies": {
		"@types/node": "22.15.22",
		"@types/react": "19.1.6",
		"@types/react-dom": "19.1.5",
		"@types/webpack": "5.28.5",
		"@types/webpack-dev-server": "4.7.2",
		"@typescript-eslint/eslint-plugin": "8.33.0",
		"@typescript-eslint/parser": "8.33.0",
		"autoprefixer": "^10.4.21",
		"css-loader": "^7.1.2",
		"eslint": "8.57.1",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-webpack-plugin": "4.2.0",
		"fork-ts-checker-webpack-plugin": "9.1.0",
		"gh-pages": "^6.3.0",
		"html-webpack-plugin": "5.6.3",
		"nodemon": "^3.1.10",
		"postcss": "^8.5.5",
		"prettier": "3.5.3",
		"style-loader": "^4.0.0",
		"tailwindcss": "^4.1.10",
		"ts-loader": "9.5.2",
		"ts-node": "10.9.2",
		"tsconfig-paths-webpack-plugin": "4.2.0",
		"typescript": "5.8.3",
		"webpack": "5.99.9",
		"webpack-cli": "6.0.1",
		"webpack-dev-server": "5.2.1"
	},
	"dependencies": {
		"@headlessui/react": "^2.2.4",
		"@hello-pangea/dnd": "^18.0.1",
		"@heroicons/react": "^2.2.0",
		"@tailwindcss/cli": "^4.1.10",
		"leaflet": "^1.9.4",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-leaflet": "^5.0.0"
	}
}
